    },
    "devDependencies": {
        "monaco-editor": "^0.40.0",
        "nodecg-io-esbuild": "^1.0.0",
        "@nodecg/types": "^2.1.11",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "crypto-js": "^4.1.1",
