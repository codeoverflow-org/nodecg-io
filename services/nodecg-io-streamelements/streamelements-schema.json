{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "jwtToken": {
            "type": "string",
            "description": "Your JWT token for streamelments."
        },
        "handleTestEvents": {
            "type": "boolean",
            "default": false,
            "description": "Whether test events should be handled."
        }
    },
    "required": ["jwtToken"]
}
