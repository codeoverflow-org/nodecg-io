{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "host": {
        "type": "string",
        "description": "An IP address to the rcon server"
      },
      "port": {
        "type": "number",
        "description": "The port of the rcon server"
      },
      "password": {
        "type": "string",
        "description": "The password of the rcon server"
      }
    },
    "required": ["host", "port", "password"]
  }